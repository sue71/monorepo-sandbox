{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base", ":enableVulnerabilityAlerts", "local>monorepo-sandbox/renovate.npm.json"],
  "labels": ["renovate", "kind/dependencies"],
  "enabledManagers": [
    "npm"
  ],
  "hostRules": [
    {
      "matchHost": "registry.npmjs.org",
      "timeout": 240000
    }
  ],
  "prConcurrentLimit": 15,
  "ignorePaths": []
}
